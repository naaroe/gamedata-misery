[smart_terrain]
def_restr = jup_a6_sr_noweap
safe_restr = jup_a6_sr_light
smart_control = smart_control
squad_id = 4
max_population = {+jup_b207_attack_started} 4, 3
respawn_params = respawn@jup_a6   
arrive_dist = 65

[respawn@jup_a6]
spawn_stalker@advanced
spawn_stalker@master

spawn_stalker@advanced_sentry

;;; unused

spawn_stalker@sci

spawn_stalker@hunter_night

; Stalker squads

[spawn_stalker@advanced]
spawn_squads = stalker_sim_squad_novice
spawn_num = {+kingpin_gained} 0, {+jup_b207_task_end} 0, {+jup_a12_task_done} 0, {+zat_b40_merc_gone} 0, {+jup_b8_heli_4_searched +zat_b101_both_heli_info} 0, {+jup_b209_task_done} 0, {+jup_b218_gather_squad_complete} 0, 1

[spawn_stalker@master]
spawn_squads = stalker_sim_squad_veteran, stalker_sim_squad_advanced_veteran, stalker_sci_simulation_squad
spawn_num = {-kingpin_gained +jup_b207_task_end} 2, {-kingpin_gained +jup_a12_task_done} 2, {-kingpin_gained +zat_b40_merc_gone} 2, {-kingpin_gained +jup_b8_heli_4_searched +zat_b101_both_heli_info} 2, {-kingpin_gained +jup_b209_task_done} 2, {-kingpin_gained +jup_b218_gather_squad_complete} 2, 0

; Stalker sentry squads

[spawn_stalker@advanced_sentry]
spawn_squads = stalker_sim_squad_advanced_jup_a6_sentry
spawn_num = 1

;;; unused

[spawn_stalker@hunter_night]
spawn_squads = stalker_sim_squad_advanced_veteran
spawn_num = 0

[spawn_stalker@sci]
spawn_squads = stalker_sci_simulation_squad
spawn_num = 0	; {+jup_b218_gather_squad_complete} 1, 0

