[smart_terrain]
squad_id = 39
max_population = 1

respawn_params = respawn@jup_sim_19

[respawn@jup_sim_19]
spawn_monolith
spawn_monolith@master

spawn_monolith@sci

spawn_controller

[spawn_monolith]
spawn_squads = monolith_sim_squad_novice
spawn_num = {+jup_b207_task_end} 0, {+jup_a12_task_done} 0, {+zat_b40_merc_gone} 0, {+jup_b8_heli_4_searched +zat_b101_both_heli_info} 0, {+jup_b209_task_done} 0, {+jup_b218_gather_squad_complete} 0, 2

[spawn_monolith@master]
spawn_squads = monolith_sim_squad_advanced, monolith_sim_squad_veteran, monolith_sci_simulation_squad
spawn_num = {+jup_b218_gather_squad_complete} 2, {+jup_b207_task_end -jup_b218_gather_squad_complete} 2, {+jup_a12_task_done -jup_b218_gather_squad_complete} 2, {+zat_b40_merc_gone -jup_b218_gather_squad_complete} 2, {+jup_b8_heli_4_searched +zat_b101_both_heli_info -jup_b218_gather_squad_complete} 2, {+jup_b209_task_done -jup_b218_gather_squad_complete} 2, 0

