[smart_terrain];jup_b46
squad_id = 25
max_population = 1

respawn_params = respawn@jup_b46

[respawn@jup_b46]
spawn_duty@advanced_sentry
spawn_duty@master_sentry

spawn_duty@advanced_sentry_hostile
spawn_duty@master_sentry_hostile

;;; unused

spawn_duty@veteran_assault
spawn_duty@veteran_assault_hostile

; Duty sentry squads

[spawn_duty@advanced_sentry]
spawn_squads = duty_sim_squad_advanced_jup_b46_sentry
spawn_num = {+sim_freedom_help_harder} 0, {+jup_b207_task_end} 0, {+jup_a12_task_done} 0, {+zat_b40_merc_gone} 0, {+jup_b8_heli_4_searched +zat_b101_both_heli_info} 0, {+jup_b209_task_done} 0, {+jup_b218_gather_squad_complete} 0, 1

[spawn_duty@master_sentry]
spawn_squads = duty_sim_squad_advanced_veteran_jup_b46_sentry
spawn_num = {-sim_freedom_help_harder +jup_b207_task_end} 1, {-sim_freedom_help_harder +jup_a12_task_done} 1, {-sim_freedom_help_harder +zat_b40_merc_gone} 1, {-sim_freedom_help_harder +jup_b8_heli_4_searched +zat_b101_both_heli_info} 1, {-sim_freedom_help_harder +jup_b209_task_done} 1, {-sim_freedom_help_harder +jup_b218_gather_squad_complete} 1, 0

; Duty sentry hostile squads

[spawn_duty@advanced_sentry_hostile]
spawn_squads = duty_sim_squad_advanced_jup_b46_sentry_hostile
spawn_num = {-sim_freedom_help_harder} 0, {+sim_freedom_help_harder +jup_b207_task_end} 0, {+sim_freedom_help_harder +jup_a12_task_done} 0, {+sim_freedom_help_harder +zat_b40_merc_gone} 0, {+sim_freedom_help_harder +jup_b8_heli_4_searched +zat_b101_both_heli_info} 0, {+sim_freedom_help_harder +jup_b209_task_done} 0, {+sim_freedom_help_harder +jup_b218_gather_squad_complete} 0, 1

[spawn_duty@master_sentry_hostile]
spawn_squads = duty_sim_squad_advanced_veteran_jup_b46_sentry_hostile
spawn_num = {+sim_freedom_help_harder +jup_b207_task_end} 1, {+sim_freedom_help_harder +jup_a12_task_done} 1, {+sim_freedom_help_harder +zat_b40_merc_gone} 1, {+sim_freedom_help_harder +jup_b8_heli_4_searched +zat_b101_both_heli_info} 1, {+sim_freedom_help_harder +jup_b209_task_done} 1, {+sim_freedom_help_harder +jup_b218_gather_squad_complete} 1, 0

;;; unused

