[smart_terrain]
squad_id = 9
max_population = 2
respawn_params = respawn@jup_b200   

[respawn@jup_b200]
spawn_freedom@advanced

spawn_freedom@veteran

spawn_freedom@sci

spawn_freedom@advanced_hostile

spawn_freedom@veteran_hostile

spawn_freedom@sci_hostile

; Freedom squads

[spawn_freedom@advanced]
spawn_squads = freedom_sim_squad_novice
spawn_num = {+sim_duty_help_harder} 0, {+jup_b207_task_end -jup_b218_gather_squad_complete} 0, {+jup_a12_task_done -jup_b218_gather_squad_complete} 0, {+zat_b40_merc_gone -jup_b218_gather_squad_complete} 0, {+jup_b8_heli_4_searched +zat_b101_both_heli_info -jup_b218_gather_squad_complete} 0, {+jup_b209_task_done -jup_b218_gather_squad_complete} 0, {+jup_b218_gather_squad_complete} 0, 2

[spawn_freedom@veteran]
spawn_squads = freedom_sim_squad_advanced_veteran, freedom_sim_squad_veteran, freedom_sci_simulation_squad
spawn_num = {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b207_task_end} 2, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_a12_task_done} 2, {-sim_duty_help_harder -jup_b218_gather_squad_complete +zat_b40_merc_gone} 2, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b8_heli_4_searched +zat_b101_both_heli_info} 2, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b209_task_done} 2, {-sim_duty_help_harder +jup_b218_gather_squad_complete} 2, 0

; Freedom hostile squads

[spawn_freedom@advanced_hostile]
spawn_squads = freedom_sim_squad_novice_hostile
spawn_num = {-sim_duty_help_harder} 0, {+sim_duty_help_harder +jup_b207_task_end -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_a12_task_done -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +zat_b40_merc_gone -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_b8_heli_4_searched +zat_b101_both_heli_info -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_b209_task_done -jup_b218_gather_squad_complete} 0, {+jup_b218_gather_squad_complete} 0, 2

[spawn_freedom@veteran_hostile]
spawn_squads = freedom_sim_squad_advanced_veteran_hostile, freedom_sim_squad_veteran_hostile, freedom_sci_simulation_squad_hostile
spawn_num = {+sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b207_task_end} 2, {+sim_duty_help_harder -jup_b218_gather_squad_complete +jup_a12_task_done} 2, {+sim_duty_help_harder -jup_b218_gather_squad_complete +zat_b40_merc_gone} 2, {+sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b8_heli_4_searched +zat_b101_both_heli_info} 2, {+sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b209_task_done} 2, {+sim_duty_help_harder +jup_b218_gather_squad_complete} 2, 0

