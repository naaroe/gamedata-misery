[smart_terrain];zat_b5
squad_id = 36
max_population = 4

respawn_params = respawn@zat_b5

arrive_dist = 150

[respawn@zat_b5]
spawn_bandit@advanced
spawn_bandit@veteran
spawn_bandit@hostile
spawn_bandit@veteran_hostile

spawn_bandit@advanced_sentry
spawn_bandit@hostile_sentry

spawn_merc@novice
spawn_merc@veteran

spawn_killer

; Bandit squads

[spawn_bandit@advanced]
spawn_squads = bandit_sim_squad_novice
spawn_num = {-kingpin_gained} 0, {+kingpin_gained +jup_b207_task_end} 0, {+kingpin_gained +jup_a12_task_done} 0, {+kingpin_gained +jup_b46_task_done} 0, {+kingpin_gained +jup_b8_heli_4_searched} 0, {+kingpin_gained +jup_b209_task_done} 0, {+kingpin_gained +jup_b218_gather_squad_complete} 0, 1

[spawn_bandit@veteran]
spawn_squads = bandit_sim_squad_advanced, bandit_sim_squad_veteran
spawn_num = {+kingpin_gained +jup_b207_task_end} 1, {+kingpin_gained +jup_a12_task_done} 1, {+kingpin_gained +jup_b46_task_done} 1, {+kingpin_gained +jup_b8_heli_4_searched} 1, {+kingpin_gained +jup_b209_task_done} 1, {+kingpin_gained +jup_b218_gather_squad_complete} 1, 0

; Bandit hostile squads

[spawn_bandit@hostile]
spawn_squads = bandit_sim_squad_hostile
spawn_num = {+kingpin_gained} 0, {-kingpin_gained +jup_b207_task_end} 0, {-kingpin_gained +jup_a12_task_done} 0, {-kingpin_gained +jup_b46_task_done} 0, {-kingpin_gained +jup_b8_heli_4_searched} 0, {-kingpin_gained +jup_b209_task_done} 0, {-kingpin_gained +jup_b218_gather_squad_complete} 0, 1

[spawn_bandit@veteran_hostile]
spawn_squads = bandit_sim_squad_advanced_hostile, bandit_sim_squad_hostile_elite
spawn_num = {-kingpin_gained +jup_b207_task_end} 1, {-kingpin_gained +jup_a12_task_done} 1, {-kingpin_gained +jup_b46_task_done} 1, {-kingpin_gained +jup_b8_heli_4_searched} 1, {-kingpin_gained +jup_b209_task_done} 1, {-kingpin_gained +jup_b218_gather_squad_complete} 1, 0

; Merc hostile squads

[spawn_merc@novice]
spawn_squads = merc_sim_squad_novice
spawn_num = {+jup_b207_task_end} 0, {+jup_a12_task_done} 0, {+jup_b46_task_done} 0, {+jup_b209_task_done} 0, {+jup_b218_gather_squad_complete} 0, 1

[spawn_merc@veteran]
spawn_squads = merc_sim_squad_advanced, merc_sim_squad_veteran, merc_sim_squad_veteran_zaton_smrter_sim_9_assault
spawn_num = {+jup_b207_task_end} 1, {+jup_a12_task_done} 1, {+jup_b46_task_done} 1, {+jup_b209_task_done} 1, {+jup_b218_gather_squad_complete} 1, 0

;;; unused

